# Continuar com as outras camadas (rodovias, anÃ¡lise de logÃ­stica, etc.)

# CAMADA: TODAS AS RODOVIAS COM ACIDENTES REGISTRADOS E ANÃLISE DE LOGÃSTICA
print("ğŸ“Š ADICIONANDO CAMADA COM TODAS AS RODOVIAS E ANÃLISE DE LOGÃSTICA...")

if coluna_br:
    # Preparar dados das rodovias
    df_enriched[coluna_br] = df_enriched[coluna_br].fillna('NÃ£o informada').astype(str)

    # Agrupar por BR
    rodovias_acidentes = df_enriched.groupby(coluna_br).agg({
        'id': 'count',
        'mortos': 'sum',
        'feridos_graves': 'sum',
        'feridos_leves': 'sum',
        'ilesos': 'sum'
    }).reset_index()

    rodovias_acidentes = rodovias_acidentes.sort_values('id', ascending=False)

    # Calcular taxa de mortalidade
    rodovias_acidentes['taxa_mortalidade'] = (rodovias_acidentes['mortos'] / rodovias_acidentes['id']) * 100
    rodovias_acidentes['taxa_mortalidade'] = rodovias_acidentes['taxa_mortalidade'].fillna(0)

    # ANÃLISE DE LOGÃSTICA PARA RODOVIAS
    rodovias_acidentes['categoria_risco'] = rodovias_acidentes['taxa_mortalidade'].apply(
        lambda x: 'MUITO ALTO' if x > 3 else 'ALTO' if x > 2 else 'MODERADO' if x > 1 else 'BAIXO'
    )

    rodovias_acidentes['recomendacao_logistica'] = rodovias_acidentes['categoria_risco'].apply(
        lambda x: 'ğŸš¨ EVITAR' if x == 'MUITO ALTO' else
                 'âš ï¸ CUIDADO EXTREMO' if x == 'ALTO' else
                 'ğŸ“‹ ATENÃ‡ÃƒO' if x == 'MODERADO' else
                 'âœ… ADEQUADO'
    )

    # Criar feature group
    fg_todas_rodovias = folium.FeatureGroup(name='Todas as Rodovias com Acidentes', show=True)

    print(f"\nğŸ›£ï¸  TOTAL DE RODOVIAS COM ACIDENTES REGISTRADOS: {len(rodovias_acidentes)}")

    rodovias_adicionadas = 0

    for idx, rodovia in rodovias_acidentes.iterrows():
        try:
            br = str(rodovia[coluna_br])

            # Limpar valores float
            if '.' in br and br.split('.')[1] == '0':
                br = br.split('.')[0]

            acidentes = int(rodovia['id'])
            mortos = int(rodovia['mortos'])
            taxa_mortalidade = float(rodovia['taxa_mortalidade'])
            recomendacao = rodovia['recomendacao_logistica']
            categoria = rodovia['categoria_risco']

            # Determinar cor
            if taxa_mortalidade > 3:
                cor_rodovia = 'darkred'
                risco_rodovia = "MUITO ALTA"
                cor_bg = "#ffebee"
            elif taxa_mortalidade > 2:
                cor_rodovia = 'red'
                risco_rodovia = "ALTA"
                cor_bg = "#fff3e0"
            elif taxa_mortalidade > 1:
                cor_rodovia = 'orange'
                risco_rodovia = "MÃ‰DIA"
                cor_bg = "#fff8e1"
            else:
                cor_rodovia = 'green'
                risco_rodovia = "BAIXA"
                cor_bg = "#e8f5e8"

            # Encontrar coordenadas para esta BR
            acidentes_br = coords_validas[coords_validas[coluna_br] == br]

            if len(acidentes_br) > 0:
                # Usar mÃ©dia das coordenadas
                lat_media = float(acidentes_br['lat_clean'].mean())
                lon_media = float(acidentes_br['lon_clean'].mean())

                popup_content = f"""
                <div style="font-family: Arial; min-width: 320px; background: {cor_bg}; padding: 15px; border-radius: 8px; border: 2px solid {cor_rodovia};">
                    <h4 style="margin: 0 0 10px 0; color: {cor_rodovia};">ğŸ›£ï¸ BR {br}</h4>
                    <div style="background: {cor_rodovia}; color: white; padding: 8px; border-radius: 4px; text-align: center; margin-bottom: 10px;">
                        <strong>PERICULOSIDADE: {risco_rodovia}</strong>
                    </div>

                    <div style="background: #2196f3; color: white; padding: 6px; border-radius: 4px; text-align: center; margin-bottom: 10px;">
                        <strong>ğŸ“¦ {recomendacao}</strong>
                    </div>

                    <table style="width: 100%; font-size: 12px;">
                        <tr><td>ğŸ“Š Total de Acidentes:</td><td style="text-align: right;"><strong>{acidentes:,}</strong></td></tr>
                        <tr><td>ğŸ’€ Mortes:</td><td style="text-align: right;"><strong>{mortos:,}</strong></td></tr>
                        <tr><td>ğŸ¥ Feridos Graves:</td><td style="text-align: right;"><strong>{int(rodovia['feridos_graves']):,}</strong></td></tr>
                        <tr><td>ğŸ©¹ Feridos Leves:</td><td style="text-align: right;"><strong>{int(rodovia['feridos_leves']):,}</strong></td></tr>
                        <tr><td>ğŸ‘¥ Ãlesos:</td><td style="text-align: right;"><strong>{int(rodovia['ilesos']):,}</strong></td></tr>
                        <tr><td>ğŸ“ˆ Taxa de Mortalidade:</td><td style="text-align: right;"><strong>{taxa_mortalidade:.2f}%</strong></td></tr>
                        <tr><td>ğŸ† Ranking Geral:</td><td style="text-align: right;"><strong>#{idx + 1}</strong></td></tr>
                        <tr><td>ğŸ“‹ Categoria:</td><td style="text-align: right;"><strong>{categoria}</strong></td></tr>
                    </table>

                    <div style="margin-top: 10px; padding: 8px; background: #e3f2fd; border-radius: 4px;">
                        <strong>ğŸšš IMPACTO LOGÃSTICO:</strong><br>
                        <span style="font-size: 11px;">
                        { "Alto impacto nas operaÃ§Ãµes - buscar alternativas" if taxa_mortalidade > 2 else
                          "Impacto moderado - avaliar custo-benefÃ­cio" if taxa_mortalidade > 1 else
                          "Baixo impacto - condiÃ§Ãµes aceitÃ¡veis para logÃ­stica" }
                        </span>
                    </div>
                </div>
                """

                folium.Marker(
                    location=[lat_media, lon_media],
                    popup=folium.Popup(popup_content, max_width=400),
                    tooltip=f"ğŸ›£ï¸ BR {br}: {acidentes:,} acidentes | {taxa_mortalidade:.2f}% mortalidade | ğŸ“¦ {recomendacao}",
                    icon=folium.Icon(color=cor_rodovia, icon='fa-road', prefix='fa')
                ).add_to(fg_todas_rodovias)

                rodovias_adicionadas += 1

        except Exception as e:
            continue

    fg_todas_rodovias.add_to(m2)
    print(f"âœ… {rodovias_adicionadas} rodovias adicionadas ao mapa com anÃ¡lise de logÃ­stica")

    # EstatÃ­sticas
    print(f"\nğŸ“ˆ ESTATÃSTICAS DAS RODOVIAS:")
    if len(rodovias_acidentes) > 0:
        top_rodovia = rodovias_acidentes.iloc[0]
        br_top = str(top_rodovia[coluna_br])
        if '.' in br_top and br_top.split('.')[1] == '0':
            br_top = br_top.split('.')[0]

        max_mortalidade_idx = rodovias_acidentes['taxa_mortalidade'].idxmax()
        rodovia_max_mortalidade = rodovias_acidentes.loc[max_mortalidade_idx]
        br_max = str(rodovia_max_mortalidade[coluna_br])
        if '.' in br_max and br_max.split('.')[1] == '0':
            br_max = br_max.split('.')[0]

        print(f"   â€¢ Rodovia com mais acidentes: BR {br_top} ({top_rodovia['id']:,} acidentes)")
        print(f"   â€¢ Rodovia com maior mortalidade: BR {br_max} ({rodovia_max_mortalidade['taxa_mortalidade']:.2f}%)")
        print(f"   â€¢ MÃ©dia de acidentes por rodovia: {rodovias_acidentes['id'].mean():.1f}")
        print(f"   â€¢ Taxa mÃ©dia de mortalidade: {rodovias_acidentes['taxa_mortalidade'].mean():.2f}%")

else:
    print("âŒ Coluna 'br' nÃ£o encontrada no dataset")

# [Continuar com o restante do cÃ³digo original...]
# Adicionar as camadas de anÃ¡lise de logÃ­stica, controles, etc.

print("\nğŸ¯ ADICIONANDO ANÃLISE DE LOGÃSTICA EM TODOS OS PONTOS...")

