# Adicionar marcadores de mortalidade por estado COM ANÃLISE DE LOGÃSTICA
print("ğŸ“ ADICIONANDO MARCADORES DE MORTALIDADE POR ESTADO COM ANÃLISE DE LOGÃSTICA...")

for _, estado in estados_acidentes.iterrows():
    uf = estado['uf']
    if uf in coordenadas_estados:
        lat, lon = coordenadas_estados[uf]

        taxa = estado['taxa_mortalidade']

        if taxa > 3:
            icon_color = 'darkred'
            risco = "MUITO ALTO"
